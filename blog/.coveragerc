[run]
branch = True
omit = blog/manage.py,
        */migrations/*,
        */tests/*,
        blog/core/settings*.py,
        blog/core/wsgi*.py,
        ../../../venv/*,

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__

[html]
directory = ../static-content/coverage